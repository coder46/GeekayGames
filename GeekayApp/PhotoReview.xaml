    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <Image x:Name="gameImg" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Left" Height="395.172" Margin="-42.606,73.712,-40.728,0" VerticalAlignment="Top" Width="483.334" UseLayoutRounding="False" d:LayoutRounding="Auto">
            <Image.RenderTransform>
                <RotateTransform  Angle="90"/>
